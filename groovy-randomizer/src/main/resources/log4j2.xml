<configuration>
    <appenders>
        <Console name="STDERR" target="SYSTEM_ERR">
            <PatternLayout pattern="[%5p] %d{HH:mm:ss}  %m %x%n"/>
        </Console>
    </appenders>
    <loggers>
        <root level="info">
            <appender-ref ref="STDERR"/>
        </root>
    </loggers>
</configuration>
